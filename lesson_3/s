    197: def computer_moves
 => 198:   binding.pry
    199:   if board.chance_of_losing?
    200:     board[board.chance_of_losing?.select(&:unmarked?)] = computer.marker
    201:   else
    202:     board[board.unmarked_keys.sample] = computer.marker
    203:   end
    204: end


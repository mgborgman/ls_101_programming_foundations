    29: def detect_winner
    30:   WINNING_LINES.each do |line|
 => 31:     binding.pry
    32:     if @squares[line[0]].marker == TTTGame::HUMAN_MARKER && @squares[line[1]].marker == TTTGame::HUMAN_MARKER &&
    33:       @squares[line[2]].marker == TTTGame::HUMAN_MARKER
    34:       return TTTGame::HUMAN_MARKER
    35:     elsif @squares[line[0]].marker == TTTGame::COMPUTER_MARKER && @squares[line[1]].marker == TTTGame::COMPUTER_MARKER &&
    36:       @squares[line[2]].marker == TTTGame::COMPUTER_MARKER
    37:       return TTTGame::COMPUTER_MARKER
    38:     end          

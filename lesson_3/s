    174:     loop do
    175:       display_board
    176:       loop do
    177:         current_player_moves
    178:         clear_screen_and_display_board
    179:         #wait
    180:         break if board.someone_won? || board.full?
    181:       end
    182:       display_round_results
    183:       wait
    184:       increment_score(board.winning_marker)
    185:       break if human.score == 5 || computer.score == 5
    186:       reset
    187:     end
    188:     display_match_results
    189:     break unless play_again?
    190:     display_play_again_message
    191:     reset
    192:     set_score_back_to_zero
    193:   end
